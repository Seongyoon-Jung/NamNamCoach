equired\r\n                />\r\n              </div>\r\n\r\n              <!-- 현재 체중 -->\r\n              <div class=\"mb-3\">\r\n                <label class=\"form-label\">현재 체중 (kg)</label>\r\n                <input\r\n                  type=\"number\"\r\n                  class=\"form-control\"\r\n                  v-model.number=\"result.weight\"\r\n                  min=\"0\"\r\n                  required\r\n                />\r\n              </div>\r\n\r\n              <!-- 목표 체중 -->\r\n              <div class=\"mb-0\">\r\n                <label class=\"form-label\">목표 체중 (kg)</label>\r\n                <input\r\n                  type=\"number\"\r\n                  class=\"form-control\"\r\n                  v-model.number=\"result.targetWeight\"\r\n                  min=\"0\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <!-- 가입 버튼 -->\r\n\r\n          <div class=\"d-flex justify-content-between mt-4\">\r\n            <button class=\"btn btn-secondary\" @click=\"prevBasic\">이전</button>\r\n            <button class=\"btn btn-primary\" @click=\"save\">회원가입</button>\r\n          </div>\r\n\r\n          <div class=\"col-12 text-center mt-4\">\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-outline-primary px-5 mx-2\"\r\n              @click=\"save\"\r\n            >\r\n              나중에 입력\r\n            </button>\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-primary px-5 mx-2\"\r\n              @click=\"save\"\r\n            >\r\n              회원가입\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <div class=\"text-center mt-4\">\r\n        <router-link\r\n          to=\"/login\"\r\n          class=\"text-decoration-none text-white-50 small\"\r\n        >\r\n          이미 계정이 있으신가요?\r\n          <span class=\"fw-semibold text-white\">로그인</span>\r\n 