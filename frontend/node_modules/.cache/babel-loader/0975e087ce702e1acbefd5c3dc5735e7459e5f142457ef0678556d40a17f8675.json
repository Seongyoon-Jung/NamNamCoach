k form-check-inline\">\r\n              <input\r\n                class=\"form-check-input\"\r\n                type=\"radio\"\r\n                name=\"gender\"\r\n                id=\"female\"\r\n                value=\"female\"\r\n              />\r\n              <label class=\"form-check-label\" for=\"female\">여성</label>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 생년월일 -->\r\n          <div class=\"col-md-6\">\r\n            <label for=\"birthdate\" class=\"form-label\">생년월일</label>\r\n            <input type=\"date\" id=\"birthdate\" class=\"form-control\" required />\r\n          </div>\r\n\r\n          <!-- 키 -->\r\n          <div class=\"col-md-3\">\r\n            <label for=\"height\" class=\"form-label\">키 (cm)</label>\r\n            <input type=\"number\" id=\"height\" class=\"form-control\" required />\r\n          </div>\r\n\r\n          <!-- 몸무게 -->\r\n          <div class=\"col-md-3\">\r\n            <label for=\"weight\" class=\"form-label\">몸무게 (kg)</label>\r\n            <input type=\"number\" id=\"weight\" class=\"form-control\" required />\r\n          </div>\r\n\r\n          <!-- 가입 버튼 -->\r\n          <div class=\"col-12 text-center mt-4\">\r\n            <button type=\"submit\" class=\"btn btn-primary px-5\">회원가입</button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <div class=\"text-center mt-4\">\r\n        <router-link\r\n          to=\"/login\"\r\n          class=\"text-decoration-none text-white-50 small\"\r\n        >\r\n          이미 계정이 있으신가요?\r\n          <span class=\"fw-semibold text-white\">로그인</span>\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<style scoped>\r\n.card {\r\n  background-color: white;\r\n  border-radius: 1rem;\r\n}\r\n\r\ninput::placeholder {\r\n  font-size: 0.9rem;\r\n  color: #999;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Signup',\r\n  data() {\r\n    return {\r\n      