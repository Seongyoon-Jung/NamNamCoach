{"ast":null,"code":"import { mapState } from 'vuex';\nexport default {\n  name: \"DashboardView\",\n  computed: {\n    ...mapState({\n      user: state => state.account // { username, avatarUrl, role, ... }\n    })\n  },\n  data() {\n    return {\n      user: {\n        username: this.$store.state.account.username\n      },\n      stats: [{\n        label: \"완료율\",\n        value: \"87%\"\n      }, {\n        label: \"활동 수\",\n        value: \"1,503\"\n      }, {\n        label: \"칼로리\",\n        value: \"2,450\"\n      }, {\n        label: \"걸음 수\",\n        value: \"4,220\"\n      }]\n    };\n  }\n};","map":{"version":3,"names":["mapState","name","computed","user","state","account","data","username","$store","stats","label","value"],"sources":["C:\\Users\\SSAFY\\Desktop\\YamYamCoach\\frontend\\src\\views\\HomeLoginView.vue"],"sourcesContent":["<template>\r\n    <div class=\"dashboard\">\r\n      <!-- Profile Section -->\r\n      <section class=\"profile-row py-4 bg-white border-bottom\">\r\n        <div class=\"container\">\r\n          <div class=\"d-flex align-items-center gap-3\">\r\n            <img\r\n              :src=\"user.avatarUrl || '/default-avatar.png'\"\r\n              alt=\"Profile\"\r\n              class=\"rounded-circle\"\r\n              width=\"60\"\r\n              height=\"60\"\r\n            />\r\n            <div>\r\n              <span class=\"mb-0 fs-2\">{{ user.username }}</span>\r\n              <span class=\"fs-5\"> 님 안녕하세요!</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n  \r\n      <!-- Stats Cards -->\r\n      <section class=\"stats-row py-4 bg-white\">\r\n        <div class=\"container\">\r\n          <div class=\"row g-3\">\r\n            <div class=\"col-md-3\" v-for=\"(stat, i) in stats\" :key=\"i\">\r\n              <div class=\"card text-center shadow-sm p-3\">\r\n                <div class=\"stat-label text-uppercase text-muted mb-2\">\r\n                  {{ stat.label }}\r\n                </div>\r\n                <div class=\"stat-value h2 fw-bold\">\r\n                  {{ stat.value }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n  \r\n      <!-- Big Charts -->\r\n      <section class=\"charts-row py-4 bg-light\">\r\n        <div class=\"container\">\r\n          <div class=\"row g-3\">\r\n            <div class=\"col-lg-8\">\r\n              <div class=\"card shadow-sm p-3\">\r\n                <div class=\"card-header\">\r\n                  <h5 class=\"mb-0\">월별 식습관 지표</h5>\r\n                </div>\r\n                <div class=\"chart-placeholder d-flex align-items-center justify-content-center\">\r\n                  <!-- 여기에 실제 LineChart 컴포넌트 삽입 -->\r\n                  차트 영역\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-4\">\r\n              <div class=\"card shadow-sm p-3\">\r\n                <div class=\"card-header\">\r\n                  <h5 class=\"mb-0\">진행률</h5>\r\n                </div>\r\n                <div class=\"chart-placeholder d-flex align-items-center justify-content-center\">\r\n                  <!-- 여기에 실제 DonutChart 컴포넌트 삽입 -->\r\n                  도넛 차트\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n  \r\n      <!-- Small Charts / Info Cards -->\r\n      <section class=\"small-charts-row py-4 bg-white\">\r\n        <div class=\"container\">\r\n          <div class=\"row g-3\">\r\n            <div class=\"col-md-3\" v-for=\"n in 4\" :key=\"n\">\r\n              <div class=\"card shadow-sm p-3\">\r\n                <div class=\"card-header\">\r\n                  <h6 class=\"mb-0\">세부 지표 {{ n }}</h6>\r\n                </div>\r\n                <div class=\"chart-placeholder small-chart d-flex align-items-center justify-content-center\">\r\n                  <!-- 여기에 실제 SmallChart 컴포넌트 삽입 -->\r\n                  작은 차트\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-3\">\r\n              <div class=\"card shadow-sm p-3 h-100\">\r\n                <div class=\"card-header\">\r\n                  <h6 class=\"mb-0\">알림 및 진행</h6>\r\n                </div>\r\n                <ul class=\"list-unstyled mb-0\">\r\n                  <li v-for=\"i in 3\" :key=\"i\" class=\"d-flex justify-content-between align-items-center py-2 border-bottom\">\r\n                    <span>알림 항목 {{ i }}</span>\r\n                    <button class=\"btn btn-sm btn-outline-primary\">보기</button>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { mapState } from 'vuex';\r\n  \r\n  export default {\r\n    name: \"DashboardView\",\r\n    computed: {\r\n      ...mapState({\r\n        user: state => state.account  // { username, avatarUrl, role, ... }\r\n      })\r\n    },\r\n    data() {\r\n      return {\r\n        user : {\r\n            username : this.$store.state.account.username\r\n        },\r\n        stats: [\r\n          { label: \"완료율\", value: \"87%\" },\r\n          { label: \"활동 수\", value: \"1,503\" },\r\n          { label: \"칼로리\",  value: \"2,450\" },\r\n          { label: \"걸음 수\", value: \"4,220\" },\r\n        ],\r\n      };\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .profile-row {\r\n    border-bottom: 1px solid #e9ecef;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAwGE,SAASA,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,QAAQ,EAAE;IACR,GAAGF,QAAQ,CAAC;MACVG,IAAI,EAAEC,KAAI,IAAKA,KAAK,CAACC,OAAM,CAAG;IAChC,CAAC;EACH,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,IAAG,EAAI;QACHI,QAAO,EAAI,IAAI,CAACC,MAAM,CAACJ,KAAK,CAACC,OAAO,CAACE;MACzC,CAAC;MACDE,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,KAAK;QAAEC,KAAK,EAAE;MAAM,CAAC,EAC9B;QAAED,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAQ,CAAC,EACjC;QAAED,KAAK,EAAE,KAAK;QAAGC,KAAK,EAAE;MAAQ,CAAC,EACjC;QAAED,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE;MAAQ,CAAC;IAErC,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}