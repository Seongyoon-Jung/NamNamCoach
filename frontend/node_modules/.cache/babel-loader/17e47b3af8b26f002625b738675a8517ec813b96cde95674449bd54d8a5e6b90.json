{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'SignupView',\n  // name은 index.js에 선언해놓은 이름과 똑같아야함\n  data() {\n    return {\n      result: {\n        email: '',\n        password: '',\n        username: '',\n        gender: null,\n        birthDate: '',\n        height: '',\n        weight: ''\n      }\n    };\n  },\n  created() {},\n  methods: {\n    save() {\n      if (this.result.email == '') {\n        console.log('이메일 입력필요');\n        return;\n      }\n      if (this.result.password == '') {\n        console.log('비밀번호 입력필요');\n        return;\n      }\n      if (this.result.username == '') {\n        console.log('닉네임 입력필요');\n        return;\n      }\n      if (this.result.gender == null) {\n        console.log('성별 입력필요');\n        return;\n      }\n      if (this.result.birthDate == '') {\n        console.log('생년월일 입력필요');\n        return;\n      }\n      if (this.result.height == '') {\n        console.log('키 입력필요');\n        return;\n      }\n      if (this.result.weight == '') {\n        console.log('몸무게 입력필요');\n        return;\n      }\n      console.log(this.result);\n      axios.post('http://localhost:8080/api/users', this.result).then(res => {\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","result","email","password","username","gender","birthDate","height","weight","created","methods","save","console","log","post","then","res","catch","err"],"sources":["C:\\SSAFY\\vue-app\\src\\views\\SignupView.vue"],"sourcesContent":["<template>\r\n  <main class=\"d-flex align-items-center justify-content-center min-vh-100\">\r\n    <div\r\n      class=\"card p-5 shadow-lg\"\r\n      style=\"max-width: 700px; width: 100%; border-radius: 1rem\"\r\n    >\r\n      <h3 class=\"text-center fw-bold mb-4 text-dark\">회원 정보 입력 (필수)</h3>\r\n      <hr />\r\n\r\n      <form>\r\n        <div class=\"row g-3\">\r\n          <!-- 이메일 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"email\" class=\"col-md-2 col-form-label\">이메일</label>\r\n              <div class=\"col-md-10\">\r\n                <div class=\"input-group\">\r\n                  <input\r\n                    type=\"email\"\r\n                    id=\"email\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"example@domain.com\"\r\n                    required\r\n                    name=\"email\"\r\n                    v-model=\"result.email\"\r\n                  />\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-outline-secondary\"\r\n                    @click=\"checkEmail\"\r\n                  >\r\n                    중복 확인\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 비밀번호 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"password\" class=\"col-md-2 col-form-label\"\r\n                >비밀번호</label\r\n              >\r\n              <div class=\"col-md-10\">\r\n                <input\r\n                  type=\"password\"\r\n                  id=\"password\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"비밀번호를 입력하세요\"\r\n                  required\r\n                  name=\"password\"\r\n                  v-model=\"result.password\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 닉네임 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"username\" class=\"col-md-2 col-form-label\"\r\n                >닉네임</label\r\n              >\r\n              <div class=\"col-md-10\">\r\n                <div class=\"input-group\">\r\n                  <input\r\n                    type=\"text\"\r\n                    id=\"username\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"사용할 닉네임\"\r\n                    required\r\n                    name=\"username\"\r\n                    v-model=\"result.username\"\r\n                  />\r\n                  <button type=\"button\" class=\"btn btn-outline-secondary\">\r\n                    중복 확인\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 성별 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label class=\"col-md-2 col-form-label\">성별</label>\r\n              <div class=\"col-md-10\">\r\n                <div\r\n                  class=\"d-flex align-items-center gap-3 border rounded px-3 py-2\"\r\n                  style=\"height: 38px\"\r\n                >\r\n                  <div class=\"form-check m-0\">\r\n                    <input\r\n                      class=\"form-check-input\"\r\n                      type=\"radio\"\r\n                      name=\"gender\"\r\n                      id=\"male\"\r\n                      v-bind:value=\"false\"\r\n                      v-model=\"result.gender\"\r\n                    />\r\n                    <label class=\"form-check-label mb-0\" for=\"male\">남성</label>\r\n                  </div>\r\n                  <div class=\"form-check form-check-inline m-0\">\r\n                    <input\r\n                      class=\"form-check-input\"\r\n                      type=\"radio\"\r\n                      name=\"gender\"\r\n                      id=\"female\"\r\n                      v-bind:value=\"true\"\r\n                      v-model=\"result.gender\"\r\n                    />\r\n                    <label class=\"form-check-label mb-0\" for=\"female\"\r\n                      >여성</label\r\n                    >\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 생년월일 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"birthdate\" class=\"col-md-2 col-form-label\"\r\n                >생년월일</label\r\n              >\r\n              <div class=\"col-md-10\">\r\n                <input\r\n                  type=\"text\"\r\n                  id=\"birthdate\"\r\n                  class=\"form-control\"\r\n                  required\r\n                  name=\"birthDate\"\r\n                  placeholder=\"예) 20000101 (8자리)\"\r\n                  v-model=\"result.birthDate\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 키와 몸무게 (나란히 배치) -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"height\" class=\"col-md-2 col-form-label\"\r\n                >키/몸무게</label\r\n              >\r\n              <div class=\"col-md-5\">\r\n                <input\r\n                  type=\"number\"\r\n                  id=\"height\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"키 (cm)\"\r\n                  required\r\n                  name=\"height\"\r\n                  v-model=\"result.height\"\r\n                />\r\n              </div>\r\n              <div class=\"col-md-5\">\r\n                <input\r\n                  type=\"number\"\r\n                  id=\"weight\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"몸무게 (kg)\"\r\n                  required\r\n                  name=\"weight\"\r\n                  v-model=\"result.weight\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 가입 버튼 -->\r\n          <div class=\"col-12 text-center mt-4\">\r\n            <!-- 일단 submit하지말고 console 출력확인을 위해 버튼으로 임시설정 -->\r\n            <button type=\"button\" class=\"btn btn-primary px-5\" @click=\"save\">\r\n              회원가입\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </form>\r\n\r\n      <div class=\"text-center mt-4\">\r\n        <router-link\r\n          to=\"/login\"\r\n          class=\"text-decoration-none text-white-50 small\"\r\n        >\r\n          이미 계정이 있으신가요?\r\n          <span class=\"fw-semibold text-white\">로그인</span>\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<style scoped>\r\n.card {\r\n  background-color: white;\r\n  border-radius: 1rem;\r\n}\r\n\r\ninput::placeholder {\r\n  font-size: 0.9rem;\r\n  color: #999;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'SignupView', // name은 index.js에 선언해놓은 이름과 똑같아야함\r\n  data() {\r\n    return {\r\n      result: {\r\n        email: '',\r\n        password: '',\r\n        username: '',\r\n        gender: null,\r\n        birthDate: '',\r\n        height: '',\r\n        weight: '',\r\n      },\r\n    };\r\n  },\r\n  created() {},\r\n  methods: {\r\n    save() {\r\n      if (this.result.email == '') {\r\n        console.log('이메일 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.password == '') {\r\n        console.log('비밀번호 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.username == '') {\r\n        console.log('닉네임 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.gender == null) {\r\n        console.log('성별 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.birthDate == '') {\r\n        console.log('생년월일 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.height == '') {\r\n        console.log('키 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.weight == '') {\r\n        console.log('몸무게 입력필요');\r\n        return;\r\n      }\r\n      console.log(this.result);\r\n\r\n      axios\r\n        .post('http://localhost:8080/api/users', this.result)\r\n        .then((res) => {\r\n          console.log(res);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAgNA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAAE;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,IAAI;QACZC,SAAS,EAAE,EAAE;QACbC,MAAM,EAAE,EAAE;QACVC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,IAAIA,CAAA,EAAG;MACL,IAAI,IAAI,CAACV,MAAM,CAACC,KAAI,IAAK,EAAE,EAAE;QAC3BU,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;QACvB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACE,QAAO,IAAK,EAAE,EAAE;QAC9BS,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;QACxB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACG,QAAO,IAAK,EAAE,EAAE;QAC9BQ,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;QACvB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACI,MAAK,IAAK,IAAI,EAAE;QAC9BO,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;QACtB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACK,SAAQ,IAAK,EAAE,EAAE;QAC/BM,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;QACxB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACM,MAAK,IAAK,EAAE,EAAE;QAC5BK,OAAO,CAACC,GAAG,CAAC,QAAQ,CAAC;QACrB;MACF;MAEA,IAAI,IAAI,CAACZ,MAAM,CAACO,MAAK,IAAK,EAAE,EAAE;QAC5BI,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;QACvB;MACF;MACAD,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,MAAM,CAAC;MAExBH,KAAI,CACDgB,IAAI,CAAC,iCAAiC,EAAE,IAAI,CAACb,MAAM,EACnDc,IAAI,CAAEC,GAAG,IAAK;QACbJ,OAAO,CAACC,GAAG,CAACG,GAAG,CAAC;MAClB,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACdN,OAAO,CAACC,GAAG,CAACK,GAAG,CAAC;MAClB,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}