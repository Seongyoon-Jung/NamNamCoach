{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from '@/plugins/axios';\nimport store from '@/store';\nexport default {\n  name: 'SignupView',\n  // name은 index.js에 선언해놓은 이름과 똑같아야함\n  data() {\n    return {\n      result: {\n        email: '',\n        password: ''\n      }\n    };\n  },\n  created() {},\n  methods: {\n    login() {\n      if (this.result.email == '') {\n        alert('이메일 입력필요');\n        return;\n      }\n      if (this.result.password == '') {\n        alert('비밀번호 입력필요');\n        return;\n      }\n\n      //  is머시기 boolean 타입으로 넘어오면 json이 강제로 surveyed로 바꿔버린다\n      axios.post('/api/auth/login', this.result).then(res => {\n        store.commit('setAccount', {\n          userId: res.data.userId,\n          username: res.data.username,\n          isSurveyed: res.data.surveyed\n        });\n        alert(\"로그인이 완료되었습니다.\");\n\n        // 설문조사를 완료했으면 홈으로\n        if (res.data.surveyed) {\n          this.$router.push({\n            name: 'HomeView'\n          });\n        }\n        //설문조사안했으면 설문조사 페이지로\n        else {\n          this.$router.push({\n            name: 'SurveyView'\n          });\n        }\n      }).catch(err => {});\n    }\n  }\n};","map":{"version":3,"names":["axios","store","name","data","result","email","password","created","methods","login","alert","post","then","res","commit","userId","username","isSurveyed","surveyed","$router","push","catch","err"],"sources":["C:\\Users\\SSAFY\\Desktop\\YamYamCoach\\frontend\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"d-flex align-items-center justify-content-center min-vh-100\">\r\n    <div\r\n      class=\"card p-5 shadow-lg\"\r\n      style=\"max-width: 450px; width: 100%; border-radius: 1rem\"\r\n    >\r\n      <h3 class=\"text-center fw-bold mb-4 text-dark\">로그인</h3>\r\n      <hr />\r\n\r\n      <form>\r\n        <div class=\"row g-3\">\r\n          <!-- 이메일 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"email\" class=\"col-md-3 col-form-label fw-bold\">이메일</label>\r\n              <div class=\"col-md-9\">\r\n                <input\r\n                  type=\"email\"\r\n                  id=\"email\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"example@domain.com\"\r\n                  name=\"email\" v-model=\"result.email\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 비밀번호 -->\r\n          <div class=\"col-md-12\">\r\n            <div class=\"row align-items-center\">\r\n              <label for=\"password\" class=\"col-md-3 col-form-label fw-bold\">비밀번호</label>\r\n              <div class=\"col-md-9\">\r\n                <input\r\n                  type=\"password\"\r\n                  id=\"password\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"비밀번호를 입력하세요\"\r\n                  name=\"password\" v-model=\"result.password\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 로그인 버튼 -->\r\n          <div class=\"col-md-12 text-center mt-4\">\r\n            <button type=\"button\" class=\"btn btn-success w-100 mb-3\" @click=\"login\">로그인</button>\r\n            <router-link class=\"btn btn-outline-primary w-100 mb-3\" to=\"/signup\">\r\n              회원가입\r\n            </router-link>\r\n          </div>\r\n\r\n          <!-- 아이디 / 비밀번호 찾기 -->\r\n          <div class=\"col-md-12 d-flex justify-content-between text-sm\">\r\n            <a href=\"#\">아이디 찾기</a>\r\n            <a href=\"#\">비밀번호 찾기</a>\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style scoped>\r\n.card {\r\n  background-color: white;\r\n  border-radius: 1rem;\r\n}\r\n\r\ninput::placeholder {\r\n  font-size: 0.9rem;\r\n  color: #999;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport axios from '@/plugins/axios';\r\nimport store from '@/store'\r\n\r\nexport default {\r\n  name: 'SignupView', // name은 index.js에 선언해놓은 이름과 똑같아야함\r\n  data() {\r\n    return {\r\n      result: {\r\n        email: '',\r\n        password: '',\r\n      },\r\n    };\r\n  },\r\n  created() {},\r\n  methods: {\r\n    login() {\r\n      if (this.result.email == '') {\r\n        alert('이메일 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.password == '') {\r\n        alert('비밀번호 입력필요');\r\n        return;\r\n      }\r\n\r\n      //  is머시기 boolean 타입으로 넘어오면 json이 강제로 surveyed로 바꿔버린다\r\n      axios\r\n        .post('/api/auth/login', this.result)\r\n        .then((res) => {\r\n          store.commit('setAccount',{userId: res.data.userId, username : res.data.username, isSurveyed : res.data.surveyed})\r\n\r\n          alert(\"로그인이 완료되었습니다.\")\r\n\r\n          // 설문조사를 완료했으면 홈으로\r\n          if(res.data.surveyed){\r\n            this.$router.push({name:'HomeView'})\r\n          }\r\n          //설문조사안했으면 설문조사 페이지로\r\n          else{\r\n            this.$router.push({name:'SurveyView'})\r\n          }\r\n          \r\n        })\r\n        .catch((err) => {\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AA2EA,OAAOA,KAAI,MAAO,iBAAiB;AACnC,OAAOC,KAAI,MAAO,SAAQ;AAE1B,eAAe;EACbC,IAAI,EAAE,YAAY;EAAE;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,IAAI,CAACL,MAAM,CAACC,KAAI,IAAK,EAAE,EAAE;QAC3BK,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MAEA,IAAI,IAAI,CAACN,MAAM,CAACE,QAAO,IAAK,EAAE,EAAE;QAC9BI,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;;MAEA;MACAV,KAAI,CACDW,IAAI,CAAC,iBAAiB,EAAE,IAAI,CAACP,MAAM,EACnCQ,IAAI,CAAEC,GAAG,IAAK;QACbZ,KAAK,CAACa,MAAM,CAAC,YAAY,EAAC;UAACC,MAAM,EAAEF,GAAG,CAACV,IAAI,CAACY,MAAM;UAAEC,QAAO,EAAIH,GAAG,CAACV,IAAI,CAACa,QAAQ;UAAEC,UAAS,EAAIJ,GAAG,CAACV,IAAI,CAACe;QAAQ,CAAC;QAEjHR,KAAK,CAAC,eAAe;;QAErB;QACA,IAAGG,GAAG,CAACV,IAAI,