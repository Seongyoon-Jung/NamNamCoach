('이메일 입력필요');\r\n        return;\r\n      }\r\n\r\n      if (this.result.password == '') {\r\n        alert('비밀번호 입력필요');\r\n        return;\r\n      }\r\n\r\n      console.log(this.result);\r\n\r\n      axios\r\n        .post('/api/auth/login', this.result)\r\n        .then((res) => {\r\n          store.commit('setAccount',res.data)\r\n          console.log(res.data);\r\n          alert(\"로그인이 완료되었습니다.\")\r\n          this.$router.push({name:'HomeView'})\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AA2EA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAAE;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG,CAAC,CAAC;EACZC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,IAAI,IAAI,CAACL,MAAM,CAACC,KAAI,IAAK,EAAE,EAAE;QAC3BK,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MAEA,IAAI,IAAI,CAACN,MAAM,CAACE,QAAO,IAAK,EAAE,EAAE;QAC9BI,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;MAEAC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,MAAM,CAAC;MAExBJ,KAAI,CACDa,IAAI,CAAC,iBAAiB,EAAE,IAAI,CAACT,MAAM,EACnCU,IAAI,CAAEC,GAAG,IAAK;QACbd,KAAK,CAACe,MAAM,CAAC,YAAY,EAACD,GAAG,CAACZ,IAAI;QAClCQ,OAAO,CAACC,GAAG,CAACG,GAAG,CAACZ,IAAI,CAAC;QACrBO,KAAK,CAAC,eAAe;QACrB,IAAI,CAACO,OAAO,CAACC,IAAI,CAAC;UAAChB,IAAI,EAAC;QAAU,CAAC;MACrC,CAAC,EACAiB,KAAK,CAAEC,GAAG,IAAK;QACdT,OAAO,CAACC,GAAG,CAACQ,GAAG,CAAC;MAClB,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}